<config xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance" xsi:noNamespaceSchemaLocation="../../../../../../vendor/magento/mtf/etc/variations.xsd">
  <testCase name="Mage\Adminhtml\Test\TestCase\CreateStoreEntityTest">
    <variation name="CreateStoreEntityTestVariation1" firstConstraint="Mage\Adminhtml\Test\Constraint\AssertStoreSuccessSaveMessage" method="test">
      <data name="store/data/group_id/dataSet" xsi:type="string">default</data>
      <data name="store/data/name" xsi:type="string">store_name_%isolation%</data>
      <data name="store/data/code" xsi:type="string">storecode_%isolation%</data>
      <data name="store/data/is_active" xsi:type="string">Enabled</data>
      <data name="config/dataSet" xsi:type="string">store_view_local_germany</data>
      <constraint name="Mage\Adminhtml\Test\Constraint\AssertStoreSuccessSaveMessage" next="Mage\Adminhtml\Test\Constraint\AssertStoreInGrid"/>
      <constraint name="Mage\Adminhtml\Test\Constraint\AssertStoreInGrid" next="Mage\Adminhtml\Test\Constraint\AssertStoreForm" prev="Mage\Adminhtml\Test\Constraint\AssertStoreSuccessSaveMessage"/>
      <constraint name="Mage\Adminhtml\Test\Constraint\AssertStoreForm" next="Mage\Adminhtml\Test\Constraint\AssertStoreBackend" prev="Mage\Adminhtml\Test\Constraint\AssertStoreInGrid"/>
      <constraint name="Mage\Adminhtml\Test\Constraint\AssertStoreBackend" next="Mage\Adminhtml\Test\Constraint\AssertStoreFrontend" prev="Mage\Adminhtml\Test\Constraint\AssertStoreForm"/>
      <constraint name="Mage\Adminhtml\Test\Constraint\AssertStoreFrontend" next="Mage\Adminhtml\Test\Constraint\AssertStoreLocalized" prev="Mage\Adminhtml\Test\Constraint\AssertStoreBackend"/>
      <constraint name="Mage\Adminhtml\Test\Constraint\AssertStoreLocalized" prev="Mage\Adminhtml\Test\Constraint\AssertStoreFrontend"/>
    </variation>
    <variation name="CreateStoreEntityTestVariation2" firstConstraint="Mage\Adminhtml\Test\Constraint\AssertStoreSuccessSaveMessage" method="test">
      <data name="store/data/group_id/dataSet" xsi:type="string">default</data>
      <data name="store/data/name" xsi:type="string">store_name_%isolation%</data>
      <data name="store/data/code" xsi:type="string">storecode_%isolation%</data>
      <data name="store/data/is_active" xsi:type="string">Disabled</data>
      <data name="config/dataSet" xsi:type="string">-</data>
      <constraint name="Mage\Adminhtml\Test\Constraint\AssertStoreSuccessSaveMessage" next="Mage\Adminhtml\Test\Constraint\AssertStoreInGrid"/>
      <constraint name="Mage\Adminhtml\Test\Constraint\AssertStoreInGrid" next="Mage\Adminhtml\Test\Constraint\AssertStoreForm" prev="Mage\Adminhtml\Test\Constraint\AssertStoreSuccessSaveMessage"/>
      <constraint name="Mage\Adminhtml\Test\Constraint\AssertStoreForm" next="Mage\Adminhtml\Test\Constraint\AssertStoreBackend" prev="Mage\Adminhtml\Test\Constraint\AssertStoreInGrid"/>
      <constraint name="Mage\Adminhtml\Test\Constraint\AssertStoreBackend" next="Mage\Adminhtml\Test\Constraint\AssertStoreNotOnFrontend" prev="Mage\Adminhtml\Test\Constraint\AssertStoreForm"/>
      <constraint name="Mage\Adminhtml\Test\Constraint\AssertStoreNotOnFrontend" prev="Mage\Adminhtml\Test\Constraint\AssertStoreBackend"/>
    </variation>
    <variation name="CreateStoreEntityTestVariation3" firstConstraint="Mage\Adminhtml\Test\Constraint\AssertStoreSuccessSaveMessage" method="test">
      <data name="store/data/group_id/dataSet" xsi:type="string">custom</data>
      <data name="store/data/name" xsi:type="string">store_name_%isolation%</data>
      <data name="store/data/code" xsi:type="string">storecode_%isolation%</data>
      <data name="store/data/is_active" xsi:type="string">Enabled</data>
      <data name="config/dataSet" xsi:type="string">store_view_local_germany</data>
      <constraint name="Mage\Adminhtml\Test\Constraint\AssertStoreSuccessSaveMessage" next="Mage\Adminhtml\Test\Constraint\AssertStoreInGrid"/>
      <constraint name="Mage\Adminhtml\Test\Constraint\AssertStoreInGrid" next="Mage\Adminhtml\Test\Constraint\AssertStoreForm" prev="Mage\Adminhtml\Test\Constraint\AssertStoreSuccessSaveMessage"/>
      <constraint name="Mage\Adminhtml\Test\Constraint\AssertStoreForm" next="Mage\Adminhtml\Test\Constraint\AssertStoreBackend" prev="Mage\Adminhtml\Test\Constraint\AssertStoreInGrid"/>
      <constraint name="Mage\Adminhtml\Test\Constraint\AssertStoreBackend" next="Mage\Adminhtml\Test\Constraint\AssertStoreFrontend" prev="Mage\Adminhtml\Test\Constraint\AssertStoreForm"/>
      <constraint name="Mage\Adminhtml\Test\Constraint\AssertStoreFrontend" next="Mage\Adminhtml\Test\Constraint\AssertStoreLocalized" prev="Mage\Adminhtml\Test\Constraint\AssertStoreBackend"/>
      <constraint name="Mage\Adminhtml\Test\Constraint\AssertStoreLocalized" prev="Mage\Adminhtml\Test\Constraint\AssertStoreFrontend"/>
    </variation>
  </testCase>
</config>
