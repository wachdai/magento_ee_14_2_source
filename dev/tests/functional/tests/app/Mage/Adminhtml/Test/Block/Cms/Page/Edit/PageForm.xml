<tabs>
    <page_information>
        <class>\Mage\Adminhtml\Test\Block\Widget\Tab</class>
        <selector>#page_tabs_main_section</selector>
        <strategy>css selector</strategy>
        <fields>
            <title />
            <identifier />
            <store_id>
                <selector>[name='stores[]']</selector>
                <input>multiselectgrouplist</input>
            </store_id>
            <is_active>
                <input>select</input>
            </is_active>
            <under_version_control>
                <input>select</input>
            </under_version_control>
        </fields>
    </page_information>
    <content>
        <class>\Mage\Adminhtml\Test\Block\Cms\Page\Edit\Tab\Content</class>
        <selector>#page_tabs_content_section</selector>
        <strategy>css selector</strategy>
        <fields>
            <content_heading />
            <content />
        </fields>
    </content>
</tabs>
