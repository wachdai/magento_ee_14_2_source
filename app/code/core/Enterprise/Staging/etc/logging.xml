<logging>
    <actions>
        <mergePost translate="label" module="enterprise_staging">
            <label>Merge Post</label>
        </mergePost>

        <unschedule translate="label" module="enterprise_staging">
            <label>Unschedule</label>
        </unschedule>
    </actions>
    <enterprise_staging translate="label">
        <label>Content Staging</label>
        <expected_models>
            <enterprise_staging__staging/>
        </expected_models>
        <actions>
            <adminhtml_staging_manage_edit>
                <action>view</action>
            </adminhtml_staging_manage_edit>

            <adminhtml_staging_manage_save>
                <action>save</action>
            </adminhtml_staging_manage_save>

            <adminhtml_staging_manage_mergePost>
                <action>mergePost</action>
            </adminhtml_staging_manage_mergePost>

            <adminhtml_staging_manage_unschedule>
                <action>unschedule</action>
            </adminhtml_staging_manage_unschedule>

            <adminhtml_staging_backup_rollbackPost>
                <action>rollback</action>
            </adminhtml_staging_backup_rollbackPost>

            <adminhtml_staging_backup_delete>
                <action>delete</action>
                <expected_models>
                    <enterprise_staging__staging_action/>
                </expected_models>
            </adminhtml_staging_backup_delete>
            <adminhtml_staging_backup_massDelete>
                <action>massDelete</action>
                <expected_models>
                    <enterprise_staging__staging_action/>
                </expected_models>
            </adminhtml_staging_backup_massDelete>
        </actions>
    </enterprise_staging>
</logging>
